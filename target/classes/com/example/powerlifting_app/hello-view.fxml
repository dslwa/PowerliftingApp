<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.powerlifting_app.HelloController">
  <padding>
    <Insets top="20" right="20" bottom="20" left="20"/>
  </padding>

  <Button fx:id="addAthleteButton" text="Add Athlete" onAction="#onAddAthleteClick"/>
  <Button fx:id="showCompetitionButton" text="Show Competition" onAction="#onShowCompetitionClick"/>
  <Button fx:id="calculatorButton" text="Calculator" onAction="#onCalculatorClick"/>
</VBox>